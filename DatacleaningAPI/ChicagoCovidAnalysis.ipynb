   "source": [
    "total_crime_with_covid_df.plot(kind='scatter',x='Cases - Total',y='Counts',color='red')"
   ]
  },
  {
   "cell_type": "raw",
   "metadata": {},
   "source": [
    "total_crime_with_covid_df.describe()"
